{
  "name": "bridge-api",
  "version": "1.0.0",
  "description": "An online marketplace to outsource physical and remote tasks ",
  "main": "server.js",
  "engines": {
    "node": "^12.13.0"
  },
  "scripts": {
    "start": "npm run build && babel-node ./src/server.js",
    "start:dev": "nodemon --exec babel-node src/server.js",
    "test": "./node_modules/.bin/nyc mocha --require @babel/register ./src/tests/**/*.test.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "migrate": "babel-node ./src/migrations/index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adebayoileri/Bridge-API.git"
  },
  "author": "Adebayo ilerioluwa",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/adebayoileri/Bridge-API/issues"
  },
  "homepage": "https://github.com/adebayoileri/Bridge-API#readme",
  "dependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.22.0",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.3",
    "nyc": "^15.1.0",
    "pg": "^7.18.2",
    "pg-connection-string": "^2.2.3",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4"
  }
}
